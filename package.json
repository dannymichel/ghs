{
  "name": "ghs-cli-tool",
  "version": "1.1.0",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && node index.js",
    "start:frontend": "cd frontend && cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "child_process": "^1.0.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "inquirer": "^9.2.23",
    "openpgp": "^5.11.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "simple-git": "^3.24.0",
    "yargs": "^17.7.2"
  }
}
